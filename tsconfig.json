{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["DOM", "ESNext"],
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"strict": true,
		"noImplicitThis": false,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}
